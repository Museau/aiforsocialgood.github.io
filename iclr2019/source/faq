<script>

  $("a[id$='_toggle']").on("click", function(){
      var id = $(this).attr('id');
      id = id.split("_")[0];
      $(".faq").hide();
      $("#"+id).show();
      $("span.label").removeClass("label-current");
      $("span.label").addClass("label-default");
      $(this).find("span").addClass("label-current");
  });



</script>



            
